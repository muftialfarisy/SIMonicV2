<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent" android:layout_height="match_parent"
    android:orientation="vertical"
    android:id="@+id/sv_profile"
    android:background="#FFFFFF">

    <LinearLayout

        android:layout_width="match_parent"

        android:layout_height="match_parent"

        android:orientation="vertical">

<LinearLayout
    android:id="@+id/ln_jdl_profile"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="@dimen/_15sdp"
    android:orientation="vertical">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Profile Individu"
        android:gravity="center"
        android:textColor="#000000"
        android:textStyle="bold"
        android:textSize="@dimen/_20sdp"/>
</LinearLayout>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/_15sdp"
        android:layout_below="@+id/ln_jdl_profile">

        <RelativeLayout
            android:id="@+id/rl_nama"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ic_profile"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:src="@drawable/ic_profile"/>
            <TextView
                android:id="@+id/nama_lengkap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_profile"
                android:text="Nama Lengkap"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_profile"
                android:layout_below="@+id/nama_lengkap">
            <TextView
                android:id="@+id/text_nama_lengkap"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_toRightOf="@id/ic_profile"
                android:layout_below="@+id/nama_lengkap"
                android:text=""/>
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_nik"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/rl_nama"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ic_nik"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:src="@drawable/ic_id_card"/>
            <TextView
                android:id="@+id/nik"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_nik"
                android:text="Nomor Induk Kependudukan (NIK)"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_nik"
                android:layout_below="@+id/nik">
                <TextView
                    android:id="@+id/text_nik"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="#000000"
                    android:layout_toRightOf="@id/ic_profile"
                    android:layout_below="@+id/nama_lengkap"
                    android:text=""/>
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_alamat_individu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/rl_nik"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ic_alamat_individu"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:src="@drawable/ic_home"/>
            <TextView
                android:id="@+id/alamat_individu"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_alamat_individu"
                android:text="Alamat Individu"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_alamat_individu"
                android:layout_below="@+id/alamat_individu">
                <TextView
                    android:id="@+id/text_alamat_individu"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/ic_alamat_individu"
                    android:layout_below="@+id/alamat_individu"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="#000000"
                    android:text=""/>
            </LinearLayout>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_alamat_karantina"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/rl_alamat_individu"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">

            <ImageView
                android:id="@+id/ic_alamat_karantina"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:layout_marginLeft="@dimen/_15sdp"
                android:src="@drawable/ic_home_karantina"/>
            <TextView
                android:id="@+id/alamat_karantina"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_alamat_karantina"
                android:text="Alamat Karantina :"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@id/ic_alamat_karantina"
                android:layout_below="@+id/alamat_karantina">
                <TextView
                    android:id="@+id/text_alamat_karantina"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@id/ic_alamat_individu"
                    android:layout_below="@+id/alamat_individu"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="#000000"
                    android:text=""/>
            </LinearLayout>
        </RelativeLayout>
            <RelativeLayout
                android:id="@+id/rl_provinsi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10sdp"
                android:layout_marginLeft="@dimen/_45sdp"
                android:layout_below="@+id/rl_alamat_karantina">
                <ImageView
                    android:id="@+id/ic_provinsi"
                    android:layout_width="@dimen/_30sdp"
                    android:layout_height="@dimen/_30sdp"
                    android:src="@drawable/ic_provinsi"/>
                <TextView
                    android:id="@+id/provinsi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Provinsi"
                    android:textSize="@dimen/_12sdp"
                    android:textStyle="bold"
                    android:layout_toRightOf="@+id/ic_provinsi"
                    android:textColor="#000000"/>
                <TextView
                    android:id="@+id/text_provinsi"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_toRightOf="@+id/ic_provinsi"
                    android:textSize="@dimen/_10sdp"
                    android:textColor="#000000"
                    android:layout_below="@+id/provinsi"
                    android:text=""/>
            </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_kota"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_45sdp"
            android:layout_below="@+id/rl_provinsi">
            <ImageView
                android:id="@+id/ic_kota"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/ic_kota"/>
            <TextView
                android:id="@+id/kota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kota/Kabupaten"
                android:layout_toRightOf="@+id/ic_kota"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/text_kota"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ic_kota"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_below="@+id/kota"
                android:text=""/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_kecamatan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_45sdp"
            android:layout_below="@+id/rl_kota">
            <ImageView
                android:id="@+id/ic_kecamatan"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/ic_kecamatan"/>
            <TextView
                android:id="@+id/kecamatan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kecamatan"
                android:textSize="@dimen/_12sdp"
                android:layout_toRightOf="@+id/ic_kecamatan"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/text_kecamatan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ic_kecamatan"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_below="@+id/kecamatan"
                android:text=""/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_kelurahan"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_45sdp"
            android:layout_below="@+id/rl_kecamatan">
            <ImageView
                android:id="@+id/ic_kelurahan"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/ic_keluarahan"/>
            <TextView
                android:id="@+id/kelurahan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Kelurahan"
                android:layout_toRightOf="@+id/ic_kelurahan"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/text_kelurahan"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ic_kelurahan"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_below="@+id/kelurahan"
                android:text=""/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_tglmulai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_below="@+id/rl_kelurahan">
            <ImageView
                android:id="@+id/ic_tglmulai"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/ic_tgl_mulai"/>
            <TextView
                android:id="@+id/tglmulai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tanggal Mulai"
                android:layout_toRightOf="@+id/ic_tglmulai"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/text_tglmulai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ic_tglmulai"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_below="@+id/tglmulai"
                android:text=""/>
        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rl_tglselesai"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/_10sdp"
            android:layout_marginLeft="@dimen/_15sdp"
            android:layout_below="@+id/rl_tglmulai">
            <ImageView
                android:id="@+id/ic_tglselesai"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_30sdp"
                android:src="@drawable/ic_tgl_selesai"/>
            <TextView
                android:id="@+id/tglselesai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Tanggal Selesai"
                android:textSize="@dimen/_12sdp"
                android:layout_toRightOf="@+id/ic_tglselesai"
                android:textStyle="bold"
                android:textColor="#000000"/>
            <TextView
                android:id="@+id/text_tglselesai"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_toRightOf="@+id/ic_tglselesai"
                android:textSize="@dimen/_10sdp"
                android:textColor="#000000"
                android:layout_below="@+id/tglselesai"
                android:text=""/>
        </RelativeLayout>
    </RelativeLayout>
    </LinearLayout>
</ScrollView>